<div id="preview-content">
	<div class="panel panel-default">
		<div class="panel-heading">
			<h1>{{t "ticket_preview" id=this.id}}</h1>
		</div>
		<div class="panel-body">
			<p>{{{sanitize this.description}}}</p>
		</div>
	</div>

	<button type="button" class="btn btn-primary btn-block" id="open-ticket">{{t "go_to_ticket"}}</button>
</div>