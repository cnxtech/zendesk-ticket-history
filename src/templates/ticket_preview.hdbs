<div id="preview-content">
	<div class="panel panel-default">
		<div class="panel-heading">
			<h1>Ticket Preview ({{#if this.type}}<span class="text-capitalize">{{this.type}}</span> {{/if}}#{{this.id}})</h1>
		</div>
		<div class="panel-body">
			<p>{{{this.description}}}</p>
		</div>
	</div>

	<button type="button" class="btn btn-primary btn-block" id="open-ticket">Open Ticket</button>
</div>